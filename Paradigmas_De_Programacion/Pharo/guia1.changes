lizeBindings.	self changed: #list.	self changed: #view.	self changed: #descriptionCode.	super changed.! !!UMLDiagramListModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/12/2010 13:56'!changed: aParameter	((aParameter = #position) or:[(aParameter = #descriptionCode)]) 		ifTrue: [				self selectedDiagram saveDescriptionIn: self diagramsHolder. 				self changed: #list]. 	((aParameter ~= #descriptionCode) and:[(aParameter  ~= #list)]) 		ifTrue:[self model changed: aParameter].	((aParameter = #selectedIndex) or:[aParameter = #position])		ifTrue:[			self changed: #descriptionCode.].	(aParameter = #view)		ifTrue:[			self diagramsHolder changed: #view].	super changed: aParameter ! !!UMLDiagramListModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/20/2010 03:38'!clearRelationshipsOfSelectedDiagramNode 	self bindingOfSelectedDiagramNode clearRelationships ! !!UMLDiagramListModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/20/2010 00:07'!diagramName 	^self selectedDiagram name! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/20/2010 04:43'!diagramNodes	^self selectedDiagram  diagramNodes ! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/20/2010 03:39'!diagramsHolder	^ diagramsHolder! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/20/2010 04:06'!diagramsHolder: anObject	diagramsHolder := anObject! !!UMLDiagramListModel methodsFor: 'binding' stamp: 'carlaGriggio 11/11/2010 21:03'!hasBindingOf: aString	^self model hasBindingOf: aString ! !!UMLDiagramListModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/20/2010 00:07'!initialize 	selectedIndex := 1.! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 11/11/2010 21:05'!initializeBindings 	self model initializeBindings ! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/20/2010 01:23'!list	^self model list! !!UMLDiagramListModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/20/2010 05:31'!listMenu: aMenu shifted: shifted	aMenu target: self model.	^self model listMenu: aMenu shifted: shifted! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/20/2010 01:21'!model	^ diagramModel ! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 11/11/2010 21:05'!model: anObject	diagramModel  := anObject.	diagramModel ifNotNil: [diagramModel removeDependent: self].	anObject ifNotNil: [anObject addDependent: self].	self initializeBindings.	selectedIndex := 1.	self changed: #list	! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 11/12/2010 14:11'!selectedDescriptionCode	^self selectedDiagram descriptionCodeOf: self selectedDiagramNode! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/20/2010 01:28'!selectedDiagram	^self model model! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/24/2010 09:59'!selectedDiagramNode	^self model selectedDiagramNode ! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/20/2010 00:07'!selectedIndex	^ selectedIndex! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/20/2010 01:53'!selectedIndex: anIndex	anIndex > 0		ifTrue: [ 			selectedIndex := anIndex.			self changed: #selectedIndex.			]! !!UMLDiagramListModel methodsFor: 'accessing' stamp: 'carlaGriggio 11/12/2010 11:49'!update: aParameter	(aParameter = #list) ifTrue:[self changed: #list].	(aParameter = #descriptionCode)		ifTrue:[self changed: #descriptionCode].	(aParameter = #view)		ifTrue:[self diagramsHolder changed: #view].				(aParameter = #selectedIndex )		ifTrue:[								self model selectedIndex: self selectedIndex. 								self changed: #list. 				self changed: #descriptionCode].	super update: aParameter.	self initializeBindings.! !!UMLDiagramListModel methodsFor: 'updating' stamp: 'carlaGriggio 9/13/2010 10:40'!updateView	self dependents do:[:dep | dep update: #view]! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/13/2010 14:51'!addDiagram: aDiagram	self model addDiagram: aDiagram.	diagrams add: aDiagram.! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/12/2010 12:22'!changed	self diagramListModel model: self selectedDiagramModel.	super changed.! !!CategoryDiagramsModel methodsFor: 'accessing' stamp: 'carlaGriggio 1/2/2011 03:20'!changed: aParameter	(aParameter == #selectedIndex)		ifTrue:[			self diagramListModel model: self selectedDiagramModel."			self diagramListModel changed: #list."			self diagramListModel changed: #descriptionCode.			self changed: #view.			"self changed: #relabel"			self changed: #diagram].	super changed: aParameter ! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'jupiter 1/3/2011 08:39'!createDiagram	|diagramName newDiagram |	((diagramName := self request: 'Diagram Name' default: '')		isNil or: [diagramName isEmpty]) ifTrue: [^self].	newDiagram:= (ClassDiagram new name: diagramName).	self addDiagram: newDiagram .	self changed: #list.	self selectedIndex: self list size.! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'jupiter 1/3/2011 09:29'!createInheritanceDiagram	|className rootClass newDiagram |	[className := self request: 'Root Class for the inheritance diagram' default: ''.	rootClass := Smalltalk at: className asSymbol]		on: Error		do: [^ UIManager default inform: className asString , ' not found'].	newDiagram := ClassDiagram inheritanceFor: rootClass.	self model addDiagram: newDiagram.	diagrams add: newDiagram.	self selectedIndex: self list size.	self changed: #list.! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/12/2010 14:59'!deleteDiagram	self model removeDiagram: self selectedDiagram .	diagrams remove: self selectedDiagram.	self changed: #list.	(self list notEmpty and:[ self selectedIndex > 1]) 		ifTrue: [self selectedIndex: self selectedIndex -1].	(self list notEmpty and:[ self selectedIndex = 1]) 		ifTrue:[self selectedIndex: 1].	self changed: #selectedIndex.	! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/20/2010 04:06'!diagramListModel	^selectedDiagramModel ifNil:[selectedDiagramModel := UMLDiagramListModel new diagramsHolder: self model; model: (self selectedDiagramModel)]! !!CategoryDiagramsModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/19/2010 20:29'!diagramNodes	^self selectedDiagram diagramNodes ! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/10/2010 02:20'!exportDiagramAsWorkspace	Workspace new saveString: self selectedDiagram descriptionCode toFile: self selectedDiagram name, '.ws' ! !!CategoryDiagramsModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/6/2010 16:52'!labelString	^self model category, ' - ', self selectedDiagram name! !!CategoryDiagramsModel methodsFor: 'accessing' stamp: 'carlaGriggio 8/19/2010 21:10'!list	^diagrams ifNil:[ diagrams:= self model diagrams]! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 10/30/2010 04:43'!listMenu: aMenu shifted: aBoolean	self list ifNotEmpty: [		aMenu addList: #(		('delete diagram'	deleteDiagram)		('rename diagram'	renameDiagram)		)].	aMenu addLine.	aMenu addList: #(('add diagram'	createDiagram)).	aMenu addList: #(('add inheritance diagram' createInheritanceDiagram)).	aMenu addLine.	aMenu addList: #(		('save diagram as PNG' saveDiagramAsPNG)		('export diagram description code as a workspace'  exportDiagramAsWorkspace)).	^ aMenu! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 1/2/2011 23:58'!renameDiagram	|diagramName currentDiagram|	diagramName := self request: 'New name for ', self selectedDiagram name default: nil.	diagramName ifNotNil: [currentDiagram := self selectedDiagram.	self deleteDiagram.	currentDiagram name: diagramName.	self addDiagram: currentDiagram.	self changed: #list.	self selectedIndex: (self list indexOf: currentDiagram).	self changed.]	! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/16/2010 13:35'!saveDiagramAsPNG	PNGReadWriter putForm: ((SUMLDiagramWhiteboard new model: self diagramListModel) update) imageForm onFileNamed: self selectedDiagram name, '.png'.! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/5/2010 02:07'!selectedDiagram	^ self list at: self selectedIndex ifAbsent:[NullDiagram new]! !!CategoryDiagramsModel methodsFor: 'accessing' stamp: 'carlaGriggio 11/12/2010 11:33'!selectedDiagramModel	^self selectedDiagram modelForDiagramsHolder: self model! !!CategoryDiagramsModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/12/2010 13:59'!update: aParameter		(aParameter = #view) ifTrue:[self changed: #view].	(aParameter = #descriptionCode) ifTrue:[self changed: #list].	self diagramListModel changed: #descriptionCode.	super update: aParameter.! !!ClassDiagramModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/20/2010 01:01'!bindingErrorBecauseOf: aVariable	^'No class description associated with the variable: ', aVariable ! !!ClassDiagramModel methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/12/2010 12:31'!changed	super changed.	self changed: #descriptionCode.	self changed: #view.! !!ClassDiagramModel methodsFor: 'menu-options' stamp: 'carlaGriggio 8/20/2010 13:36'!chooseClassFromCategory	| dialog choice |	dialog := PopupChoiceOrRequestDialogWindow new filterInput: false;		title: 'Choose a class from the category ', self category;		labels: (SystemOrganization classesInCategory: self category);		lines: #();		model: (SystemOrganization classesInCategory: self category).	choice := (SystemWindow topWindow openModal: dialog) choice.	dialog cancelled ifTrue: [ ^nil ].	choice 		ifNotNil: [ ^"self checkBoxName: "choice ]		ifNil: [ ^dialog filterValue ]! !!ClassDiagramModel methodsFor: 'menu-options' stamp: 'carlaGriggio 8/19/2010 09:08'!chooseTraitFromCategory	| dialog choice |	dialog := PopupChoiceOrRequestDialogWindow new filterInput: false;		title: 'Choose a trait from the category ', self category;		labels: (SystemOrganization orderedTraitsIn: self category);		lines: #();		model: (SystemOrganization orderedTraitsIn: self category).	choice := (SystemWindow topWindow openModal: dialog) choice.	dialog cancelled ifTrue: [ ^nil ].	choice 		ifNotNil: [ ^self checkBoxName: choice ]		ifNil: [ ^dialog filterValue ]! !!ClassDiagramModel methodsFor: 'menu-options' stamp: 'carlaGriggio 11/12/2010 11:55'!createClassBox	| className |	className := self request: 'Class Name' default: ''.	self checkBoxName: className.	self model addNewClassDescriptionNamed: className.	self changed.! !!ClassDiagramModel methodsFor: 'menu-options' stamp: 'carlaGriggio 8/19/2010 09:08'!createClassBoxFromCategory	self createClassBoxFromClass: self chooseClassFromCategory.	self changed.! !!ClassDiagramModel methodsFor: 'menu-options' stamp: 'carlaGriggio 8/19/2010 09:08'!createClassBoxFromClass	|className|	className := self request: 'For which class would you like to create a new Class Box?' default: nil.	self checkBoxName: className.	className ifNotNil:[self createClassBoxFromClass: (Smalltalk at: className asSymbol ).	self changed.]! !!ClassDiagramModel methodsFor: 'menu-options' stamp: 'carlaGriggio 11/11/2010 21:08'!createClassBoxFromClass: aClass	aClass ifNotNil: [ self model addDiagramNode: (ClassBox fromClass: aClass )].	self changed.! !!ClassDiagramModel methodsFor: 'menu-options' stamp: 'carlaGriggio 11/12/2010 20:48'!createInterfaceBox	| interfaceName |	interfaceName := self request: 'Interface / Type Name' default: ''.	self checkBoxName: interfaceName .	self model addDiagramNode: (InterfaceBox newNamed: interfaceName).	self changed.! !!ClassDiagr