g ]]! !!NCCurve methodsFor: 'accessing contours' stamp: 'nk 8/18/2004 10:26'!boundingBox	"return my actual bounding box"	| rect |	rect := Rectangle encompassing: (Array streamContents: [ :s |		self bezierSegmentsDo: [ :seg | seg lineSegmentsDo: [ :p1 :p2 | s nextPut: p1; nextPut: p2 ]]]).	^rect! !!NCCurve methodsFor: 'accessing' stamp: 'nk 2/3/2004 17:24'!bounds	^bounds! !!NCCurve methodsFor: 'accessing' stamp: 'nk 9/2/2004 09:22'!bounds: aRectangle	self updateBounds.	self scale: (self scale * ((aRectangle width / (bounds width max: 1)) @ (aRectangle height / (bounds height max: 1))))! !!NCCurve methodsFor: 'accessing contours' stamp: 'nk 2/4/2004 08:40'!contourPoints	"Answer my contours flattened as a series of points."	^self contourPointsCollect: [ :ea | ea ].! !!NCCurve methodsFor: 'accessing contours' stamp: 'nk 8/17/2004 15:18'!contourPointsCollect: aBlock 	^ Array		streamContents: [:s | self contours				do: [:c | c						do: [:p | s								nextPut: (aBlock value: p)]]]! !!NCCurve methodsFor: 'accessing contours' stamp: 'nk 2/4/2004 08:38'!contourPointsDo: aBlock 	self contours do: [:c | c do: aBlock]! !!NCCurve methodsFor: 'accessing contours' stamp: 'nk 2/4/2004 08:38'!contourPointsTransformedBy: aTransform	^self contourPointsCollect: [ :p | aTransform transformPoint: p ].! !!NCCurve methodsFor: 'accessing' stamp: 'nk 2/3/2004 17:25'!contours	^contours! !!NCCurve methodsFor: 'accessing' stamp: 'nk 2/3/2004 17:20'!contours: anObject	contours := anObject! !!NCCurve methodsFor: 'accessing contours' stamp: 'nk 8/17/2004 15:15'!contoursTransformedFor: aRectangle rotation: rotation	| transform |	transform := self transformForRect: aRectangle rotation: rotation.	^contours collect: [ :c | transform localPointsToGlobal: c ]! !!NCCurve methodsFor: 'as yet unclassified' stamp: 'nk 8/18/2004 13:28'!curveInfoNearest: aPoint 	"Answer the index of the segment nearest aPoint, along with the closest point, its distance, and the parametric value. "	| closestPoint minDist minT segmentIndex i |	closestPoint := minDist := nil.	i := 0.	self bezierSegmentsDo:  			[:seg |			i := i + 1. 			seg lineSegmentsWithParametricValueDo: 					[:p1 :p2 :t1 :t2 | 					| curvePoint dist |					curvePoint := aPoint nearestPointOnLineFrom: p1 to: p2.					dist := curvePoint dist: aPoint.					(closestPoint isNil or: [dist < minDist]) 						ifTrue: 							[closestPoint := curvePoint.							minDist := dist.							segmentIndex := i.							minT := (p1 = p2) ifTrue: [ t1 ] ifFalse: [ t1 interpolateTo: t2 at: (curvePoint dist: p1) / (p1 dist: p2)] ]]].	^{ closestPoint. segmentIndex. minDist. minT }! !!NCCurve methodsFor: 'accessing' stamp: 'nk 2/4/2004 08:40'!defaultScale: floatOrPoint	"scale me as indicated, then call my current scale the default"	self scale: floatOrPoint; setAsDefaultScale.! !!NCCurve methodsFor: 'drawing' stamp: 'nk 2/4/2004 08:38'!drawOn: aCanvas fgColor: fgColor 	| canvas |	canvas := aCanvas asBalloonCanvas.	canvas aaLevel: 4.	canvas 		drawGeneralBezierShape: self contours		color: nil		borderWidth: 1		borderColor: fgColor.! !!NCCurve methodsFor: 'drawing' stamp: 'nk 8/17/2004 15:34'!drawOn: aCanvas in: aRectangle bgColor: bgColor fill: fillColor borderWidth: bw borderColor: bc 	self drawOn: aCanvas in: aRectangle rotatedBy: 0.0 fill: fillColor borderWidth: bw borderColor: bc ! !!NCCurve methodsFor: 'drawing' stamp: 'nk 8/17/2004 15:35'!drawOn: aCanvas in: aRectangle fill: fillColor borderWidth: bw borderColor: bc 	self drawOn: aCanvas in: aRectangle rotatedBy: 0.0 fill: fillColor borderWidth: bw borderColor: bc ! !!NCCurve methodsFor: 'drawing' stamp: 'nk 8/17/2004 18:47'!drawOn: aCanvas in: aRectangle rotatedBy: rotation fill: fillColor borderWidth: bw borderColor: bc 	aCanvas		drawGeneralBezierShape: (self contoursTransformedFor: aRectangle rotation: rotation)		color: fillColor		borderWidth: bw		borderColor: bc! !!NCCurve methodsFor: 'accessing' stamp: 