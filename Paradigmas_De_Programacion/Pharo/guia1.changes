nNode: node]"]]! !!DiagramWhiteboard methodsFor: 'positioning' stamp: 'carlaGriggio 10/26/2010 01:33'!positionNode: aNode	aNode position: self availablePositions anyOne.! !!DiagramWhiteboard methodsFor: 'positioning' stamp: 'carlaGriggio 10/24/2010 18:21'!positionedDiagramNodes	^self diagramNodes select:[:node | node position notNil]! !!DiagramWhiteboard methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 12/17/2010 02:19'!update	self removeAllMorphs.	diagramNodes := Dictionary new.	self positionDiagramNodes.	self diagramNodeMorphs" do: [:classBoxMorph | classBoxMorph updateRelationships  ]".	diagramNodes keysAndValuesDo: [:classBoxDescription :classBoxMorph | 		(classBoxDescription relations" difference: classBoxMorph connections") do:[:relation | 			relation view fitIntoDiagram:self ]].	self addAllMorphs: (self diagramNodeMorphs difference: submorphs ). 	self diagramNodeMorphs do: #adjustPosition.		self diagramNodeMorphs do: [:classBoxMorph | classBoxMorph updateBounds  ].	self bounds: self fullBounds.	self position: 0@0.	self connections do: #updateView.	self connections do: #updateLabel.	"owner ifNil:[self bounds: self fullBounds] ifNotNil:[self bounds: owner bounds]."	! !!DiagramWhiteboard methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/12/2010 20:59'!update: aParameter	(aParameter == #view) 		ifTrue:[self update.].! !!DiagramWhiteboard methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 9/2/2010 09:08'!updateDescriptionCode 	self model update: #descriptionCode.! !!DiagramWhiteboard methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 5/4/2012 02:16'!updateRelationships				self diagramNodeMorphs do: #resetBoundings.		self connections do:[:rel | rel fitIntoDiagram: self].		self diagramNodeMorphs do: #updateBounds.! !!SingleConnectionPoint methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 8/13/2010 20:04'!attachHeadOf: aConnector 	(inConnections includes: aConnector) ifFalse:[ inConnections add: a