 			[:c :ci | 			aStream				cr;				nextPutAll: 'at: ';				print: ci;				nextPutAll: ' put: '.			(c collect: [:p | (p - self offset) / self scale roundTo: 0.001]) 				storeOn: aStream.			aStream nextPut: $;].	aStream		nextPutAll: ' yourself))';		cr.	aStream		nextPutAll: 'scale: ';		print: (self scale roundTo: 0.01);		nextPutAll: ';';		cr.	aStream		nextPutAll: 'offset: ';		print: (self offset roundTo: 0.001);		nextPutAll: ';';		cr.	aStream		nextPutAll: 'yourself)';		cr! !!NCCurve methodsFor: 'drawing' stamp: 'nk 2/4/2004 08:49'!transformForRect: outsideRect rotation: angle 	| requiredScale newRect |	newRect := self bounds scaleAndTranslateToFitInside: outsideRect.	requiredScale := newRect extent asFloatPoint / self bounds extent.	^((((MatrixTransform2x3 withOffset: outsideRect center) 		composedWithLocal: (MatrixTransform2x3 withRotation: angle)) 			composedWithLocal: (MatrixTransform2x3 withOffset: outsideRect extent / -2)) 			composedWithLocal: (MatrixTransform2x3 withScale: requiredScale asPoint)) 			composedWithLocal: (MatrixTransform2x3 					withOffset: self bounds origin negated)! !!NCCurve methodsFor: 'accessing' stamp: 'nk 8/18/2004 10:28'!updateBounds	| boundsRect |	boundsRect := self boundingBox.	boundsRect width isZero 		ifTrue: [boundsRect := boundsRect expandBy: (self scale x / 20.0) @ 0.0].	boundsRect height isZero 		ifTrue: [boundsRect := boundsRect expandBy: 0.0 @ (self scale y / 20.0)].	bounds := boundsRect! !!NCCurve methodsFor: 'initialize-release' stamp: 'FernandoOlivero 4/12/2010 00:40'!withContours: anArray	scale := 1.0 @ 1.0.	offset := 0.0 @ 0.0.	self contours: (anArray collect: [:a | a collect: [:p | p asFloatPoint]]).	self updateBounds! !!NCLineArrowGlyph class methodsFor: 'arrow forms' stamp: 'FernandoOlivero 5/18/2010 14:39'!appleWithBite	"glyph 210 from BitstreamVeraSans"	^ (NCLineArrowGlyph withContours: ((Array new: 2) at: 1 put: ((Array new: 66) at: 1 put: 0.0 @ 2.059;				 at: 2 p