sReferencesTo: anObjectBrowserObject	^self variables anySatisfy: [ :v | v hasReferencesTo: anObjectBrowserObject ].! !!LOOPLesson methodsFor: 'as yet unclassified' stamp: 'GuillermoPolito 8/18/2010 19:35'!hasVariable: aVariableName	^self variables anySatisfy: [:v | v name = aVariableName ].! !!LOOPLesson methodsFor: 'as yet unclassified' stamp: 'GuillermoPolito 8/20/2010 23:44'!hasVariableNamed: aVariableName	^self variables anySatisfy: [:v | v name = aVariableName ].! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 10/27/2010 19:15'!initialize	executionContext := self loopExecutionContextClass new							lesson: self;							yourself.	objectDiagram := LOOPObjectDiagramFactory buildDiagram lesson: self; yourself.! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 9/14/2011 00:04'!lessonEnvironment	^lessonEnvironment ifNil: [ 		lessonEnvironment := SystemDictionary withOrganizer: SystemOrganizer new.		 ].! !!LOOPLesson methodsFor: 'exportation' stamp: 'GiselaDecuzzi 9/10/2010 18:32'!lessonIdentification	^self title asLegalSelector! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 9/13/2011 23:54'!loopExecutionContextClass	^loopExecutionContextClass ifNil: [ 		loopExecutionContextClass :=  			self createClass: #LessonExecutionContext 				subclassOf: LOOPExecutionContext. ]! !!LOOPLesson methodsFor: 'showing' stamp: 'GiselaDecuzzi 9/2/2012 21:44'!menuActions^OrderedCollection		with: (Array braceWith: 'abrir' translated with: #openSelectedLesson)		with: (Array braceWith: 'eliminar' translated with: #removeSelectedLesson)		with: (Array braceWith: 'renombrar' translated with: #renameSelectedLesson)		with: (Array braceWith: 'exportar' translated with: #fileOutSelectedLesson)! !!LOOPLesson methodsFor: 'showing' stamp: 'GuillermoPolito 9/28/2011 18:57'!objectBrowserObjectForRealObject: realObject	^self objects detect: [ :o | o realObject == realObject ] ifNone: [ NoObjectBrowserObject for: realObject ].! !!LOOPLesson methodsFor: 'diagram' stamp: 'carlaGriggio 10/26/2010 01:36'!objectDiagram	^objectDiagram ! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 12/2/2010 23:51'!objects	^objects ifNil: [objects := Set new].! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 7/2/2010 22:51'!objects: anObject	objects := anObject! !!LOOPLesson methodsFor: 'showing' stamp: 'GuillermoPolito 9/11/2010 17:47'!openInWorld	browser := (ObjectBrowser onLesson: self) openInWorld! !!LOOPLesson methodsFor: 'accessing' stamp: 'Aivan 9/5/2012 15:11'!parseTestCodeFrom: stream! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 9/11/2010 17:35'!printString	^self title! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 11/16/2010 20:46'!realObjects	^self objects collect: [ :o | o realObject ].! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 10/27/2010 16:12'!referencedObjects	| referencedObjects |	self deprecated: 'use self buildGraph objects'.	referencedObjects := Set new.	self variables do:[ :v | v pointsTo fullfillObjectBrowserObjectsCollection: referencedObjects ].	^referencedObjects! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 7/2/2010 23:43'!referencesObject: anObjectBrowserObject	^self variables anySatisfy: [:v | v isPointingTo: anObjectBrowserObject ].! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 10/27/2010 19:10'!removeDerrefencedObject: aDereferencedObject 	self deprecated: 'no se usa'.	self dereferencedObjects remove: aDereferencedObject .! !!LOOPLesson methodsFor: 'as yet unclassified' stamp: 'GuillermoPolito 8/22/2013 17:39'!removeFromSystem	self loopExecutionContextClass removeFromSystem.	self lessonEnvironment do: [ :c | c isBehavior ifTrue: [ c removeFromSystem ] ].	self lessonEnvironment removeAll.! !!LOOPLesson methodsFor: 'as yet unclassified' stamp: 'GuillermoPolito 7/2/2010 23:44'!removeObject: anObjectBrowserObject 	self objects remove: anObjectBrowserObject.! !!LOOPLesson methodsFor: 'as yet unclassified' stamp: 'carlaGriggio 11/5/2010 03:32'!removeTest: aTest	tests removeKey: aTest selector! !!LOOPLesson methodsFor: 'as yet unclassified' stamp: 'GuillermoPolito 8/22/2013 17:46'!removeVariable: anOBVariable 	self variables remove: anOBVariable .	self suspendAllWhile: [		self executionContext class removeInstVarNamed: anOBVariable name.	]! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 11/5/2010 17:07'!runAllTests	| results result |	results := LOOPTestCaseResult new.	self tests do: [ :t | 		results add:( t run ).		].	^results! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 7/2/2010 23:39'!runGarbageCollector 	self dereferencedObjects do: [:o | 		o removeClassFromSystem.		self removeObject: o ].! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 10/27/2010 19:09'!selectFrom: start to: end 	"asco para poder hacer fileins"! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 9/7/2010 21:39'!selectionInterval	"asco para poder hacer fileins"	^1 to: 0! !!LOOPLesson methodsFor: 'system-events' stamp: 'GuillermoPolito 8/22/2013 17:45'!suspendAllWhile: aBlock	"we suspend the system announcements"	self class suspendAllWhile: aBlock! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 10/19/2010 20:21'!testDictionary	^tests ifNil: [ tests := Dictionary new ]! !!LOOPLesson methodsFor: 'accessing' stamp: 'GuillermoPolito 10/19/2010 20:36'!tests	^self testDictionary values! !!LOOPLesson methodsFor: 'accessing' stamp: 'cfg 6/25/2010 11:33'!title	^ title! !!LOOPLesson methodsFor: 'accessing' stamp: 'cfg 6/25/2010 11:33'!title: anObject	title := anObject! !!LOOPLesson methodsFor: 'diagram' stamp: 'carlaGriggio 10/27/2010 20:17'!updateObjectDiagram	objectDiagram update! !!LOOPLesson methodsFor: 'as yet unclassified' stamp: 'GuillermoPolito 8/18/2010 19:36'!variableNamed: aByteString 	^self variables detect:[ 